&lt;table class="t_forms" cellspacing="0" cellpadding="2"&gt;
&lt;tr&gt;&lt;td&gt;
    &lt;FORM id="FORM1" name="FORM1"&gt;
        &lt;select name = "selWSSS" align="center" style="font-size:11pt"&gt;
            &lt;option value="1"&gt;足し算&lt;/option&gt;  &lt;option value="2"&gt;引き算&lt;/option&gt;
            &lt;option value="3"&gt;掛け算&lt;/option&gt;
            &lt;option value="4"&gt;割り算&lt;/option&gt;  
        &lt;/select&gt;                       
        &lt;select name = "selKeta" align="center"         style="font-size:11pt"&gt;
            &lt;option value="11"&gt;１桁と１桁&lt;/option&gt;
            &lt;option value="21"&gt;２桁と１桁&lt;/option&gt;
            &lt;option value="22"&gt;２桁と２桁&lt;/option&gt;
            &lt;option value="31"&gt;３桁と１桁&lt;/option&gt;
            &lt;option value="32"&gt;３桁と２桁&lt;/option&gt;
        &lt;/select&gt;                       
&lt;br /&gt;&lt;br /&gt;
    &lt;input type="checkbox" id="chkAmari" name="chkAmari"&gt;
    &lt;label for="chkAmari"&gt;割り算のとき答えに余りがない問題のみ出題する&lt;/label&gt;                
